<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- Define Beans -->
	<bean id="myMoney" class="com.udemy.application.onWhat">
	</bean>
	<bean id="myHero" class="com.udemy.application.IronMan" scope = "singleton">
		<constructor-arg ref="myMoney"></constructor-arg>
	</bean>
	
	<bean id="commonMan" class="com.udemy.application.CheckScope" scope="prototype">
		<constructor-arg ref="myMoney"/>
	</bean>
	


</beans>